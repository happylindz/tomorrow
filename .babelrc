{
  "presets": [["env", { 
    "modules": false,       
    "targets": {
      "browsers": "defaults"
    },
    "useBuiltIns": true 
  }], "react"],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-class-properties",
    ["import", {
      "libraryName": "antd"
    }],
    "react-hot-loader/babel",
    "syntax-dynamic-import"
  ]
}